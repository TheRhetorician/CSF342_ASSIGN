input_till_0:  
        ipa

        jmpn input_till_0
        jmpz if_input_0

        m2m dataloc

        lda dataloc
        add one
        sta dataloc

        jmp input_till_0

if_input_0:  
        lda dataloc
        sub one
        sta dataloc

output_in_LIFO:  
        m2a dataloc
        opa

        lda dataloc
        sub one
        sta dataloc

        lda dataloc
        sub offset_dataloc
        jmpn done

        jmp output_in_LIFO
done:
        stop

dataloc:  .data 1 70
offset_dataloc:  .data 1 70
one:    .data 1 1